{
	"name": "Dark June",
	"type": "dark",
	"colors": {
		"editor.background": "#202020", // primary dark colour.
		"editor.foreground": "#C2C2C2", // primary light colour.

		// Side bar is where the files, extensions etc are.
		"sideBarTitle.foreground": "#BBBBBB",
		"sideBar.background": "#202020",
		"sideBar.border": "#303030",
		
		// Activity bar is the left-most (or right-most) bar.
		"activityBarBadge.background": "#D52090",
		"activityBar.foreground": "#D0D0D0",
		"activityBar.activeBorder": "#D0D0D0",
		"activityBar.background": "#202020",
		"activityBar.border": "#303030"
	},
	// #D52090 - badge colour, delete, warning
	// #C8C8C8 - lighter white colour
	// #828282 - darker white colour
	// #CC9292 - orange
	// #B981B4 - pink
	// #AF92CC - purple
	// #7FAB7B - green
	// #E4D4A5 - yellow
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#6D6D6D"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#FBE3BF"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#FDB082"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#FF8942"
			}
		},
		{
			"scope": "constant.character, constant.other",
			"settings": {
				"foreground": "#FDB082"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#E9FDAC"
			}
		},
		{
			"scope": "keyword",
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"foreground": "#FF5E5E",
				"fontStyle": ""
			}
		},
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#FBDFB5",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.class",
			"settings": {
				"foreground": "#8CDAFF",
				"fontStyle": "underline"
			}
		},
		{
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#8CDAFF",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#8CDAFF",
				"fontStyle": ""
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"foreground": "#FC9354",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#FF5E5E",
				"fontStyle": ""
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#97D8EA",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.function",
			"settings": {
				"foreground": "#6699CC",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.constant",
			"settings": {
				"foreground": "#ECFDB9",
				"fontStyle": ""
			}
		},
		{
			"scope": "support.type, support.class",
			"settings": {
				"foreground": "#FBE3BF"
			}
		},
		{
			"scope": "support.other.variable",
			"settings": {
				"fontStyle": ""
			}
		},
		{
			"scope": "support.other.namespace, entity.name.type.namespace",
			"settings": {
				"foreground": "#FFB2F9"
			}
		},
		{
			"scope": "support.other.namespace.use-as.php",
			"settings": {
				"foreground": "#66D9EF"
			}
		},
		{
			"scope": "variable.language.namespace.php",
			"settings": {
				"foreground": "#D66990"
			}
		},
		{
			"scope": "punctuation.separator.inheritance.php",
			"settings": {
				"foreground": "#F92672"
			}
		},
		{
			"scope": "support.function.misc.css, support.constant.property-value.css, support.constant.font-name.css",
			"settings": {
				"foreground": "#FDB082"
			}
		},
		{
			"scope": "meta.tag.template.value.twig, meta.tag.template.block.twig",
			"settings": {
				"foreground": "#CD5AC5"
			}
		},
		{
			"scope": "keyword.control.twig",
			"settings": {
				"foreground": "#E05D8C"
			}
		},
		{
			"scope": "variable.other.twig",
			"settings": {
				"foreground": "#E5A5E0"
			}
		},
		{
			"scope": "variable.other.property.twig",
			"settings": {
				"foreground": "#FFE1FC"
			}
		},
		{
			"scope": "constant.language.twig",
			"settings": {
				"foreground": "#FFD2A6"
			}
		},
		{
			"scope": "constant.numeric.twig",
			"settings": {
				"foreground": "#FFD0FB"
			}
		},
		{
			"scope": "support.function.twig",
			"settings": {
				"foreground": "#90E7F7"
			}
		},
		{
			"scope": "meta.function-call.other.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "meta.function-call.twig",
			"settings": {
				"foreground": "#FAB85A"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#F92649",
				"fontStyle": ""
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#F8F8F0",
				"background": "#FF9664"
			}
		},
		{
			"scope": "punctuation.definition.string, punctuation.definition.variable, punctuation.definition.string, punctuation.definition.parameters, punctuation.definition.string, punctuation.definition.array",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"scope": "variable.parameter.function",
			"settings": {
				"foreground": "#D0D0D0"
			}
		},
		{
			"scope": "entity.other.attribute-name.id, punctuation.definition.entity",
			"settings": {
				"foreground": "#66A9EC"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6796E6"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#F44747"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#B267E6"
			}
		}
	]
}