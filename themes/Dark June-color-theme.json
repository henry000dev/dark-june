{
	"name": "Dark June",
	"type": "dark",
	"colors": {
		// The most important bit!
		"editor.background": "#202020", // primary dark colour.
		"editor.foreground": "#B8B8B8", // primary light colour.

		// Side bar is where the files, extensions etc are.
		"sideBarTitle.foreground": "#BBBBBB",
		"sideBar.background": "#202020",
		"sideBar.border": "#303030",
		"sideBar.foreground": "#B8B8B8",

		// Tabs in the editor.
		"tab.activeForeground": "#B8B8B8",

		// Menu.
		"menu.foreground": "#B8B8B8",
		
		// Activity bar is the left-most (or right-most) bar.
		"activityBarBadge.background": "#D52090",
		"activityBar.foreground": "#D0D0D0",
		"activityBar.activeBorder": "#D0D0D0",
		"activityBar.background": "#202020",
		"activityBar.border": "#303030",

		// Status bar.
		"statusBar.background": "#3C3C3C",
		"statusBar.foreground": "#B8B8B8",
		"statusBar.debuggingBackground": "#D52090",

		// Title bar.
		"titleBar.activeBackground": "#3C3C3C"
	},
	// #D52090 - badge colour, delete, warning
	// #B83030 - error
	// #B8B8B8 - lighter white colour
	// #777777 - darker white colour
	// #AF92CC - indigo
	// #CC9292 - pink
	// #97AF96 - green
	// #C9B888 - yellow
	// #88A2D3 - blue

	// Types: blue
	// Annotations: yellow
	// Function/Method params: pink
	"tokenColors": [
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#B8B8B8"
			}
		},
		{
			"scope": "keyword, storage.type, storage.modifier",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "storage.type, storage.modifier",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "punctuation.definition.tag.html, punctuation.definition.tag.begin.html, punctuation.definition.tag.end.html",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "keyword.operator, constant.other.color, punctuation, meta.tag, punctuation.definition.tag, punctuation.separator.inheritance.php, punctuation.section.embedded, keyword.other.template, keyword.other.substitution",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "entity.name.tag, punctuation.definition.entity.css, entity.other.attribute-name.id.css, entity.other.attribute-name.class.css, meta.tag.sgml, markup.deleted.git_gutter",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "entity.name.function, meta.function-call, variable.function, support.function, keyword.other.special-method, meta.block-level",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "support.other.variable, string.other.link",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "constant.numeric, constant.language, support.constant, constant.character, variable.parameter, keyword.other.unit",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "constant.other.symbol, constant.other.key, entity.other.inherited-class, markup.heading, markup.inserted.git_gutter, meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js",
			"settings": {
				"foreground": "#CC9292",
			}
		},
		{
			"scope": "string, punctuation.definition.string.begin, punctuation.definition.string.end",
			"settings": {
				"foreground": "#B8B8B8",
			}
		},
		{
			"scope": "entity.name.class, entity.name.type.class, support.type, support.class, support.orther.namespace.use.php, meta.use.php, support.other.namespace.php, markup.changed.git_gutter",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "entity.name.module.js, variable.import.parameter.js, variable.other.class.js",
			"settings": {
				"foreground": "#97AF96"
			}
		},
		{
			"scope": "variable.language",
			"settings": {
				"foreground": "#88A2D3",
			}
		},
		{
			"scope": "entity.name.method.js",
			"settings": {
				"foreground": "#B8B8B8"
			}
		},
		{
			"scope": "meta.class-method.js entity.name.function.js, variable.function.constructor",
			"settings": {
				"foreground": "#B8B8B8"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#97AF96"
			}
		},
		{
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#D52090"
			}
		},
		{
			"scope": "markup.changed",
			"settings": {
				"foreground": "#88A2D3"
			}
		},
		{
			"scope": "string.regexp",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "*url*, *link*, *uri*",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "constant.numeric.line-number.find-in-files - match",
			"settings": {
				"foreground": "#88A2D3"
			}
		},
		{
			"scope": "entity.name.filename.find-in-files",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "tag.decorator.js entity.name.tag.js, tag.decorator.js punctuation.definition.tag.js",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "source.js constant.other.object.key.js string.unquoted.label.js",
			"settings": {
				"foreground": "#97AF96"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#88A2D3"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#D52090"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#B83030"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#88A2D3"
			}
		},

		// SQL specifics.
		{
			"scope": "source.sql, support.function.security.sql",
			"settings": {
				"foreground": "#CC9292"
			}
		},

		// JavaScript language keywords.
		{
			"scope": "constant.language.null.js, constant.language.undefined.js, variable.language.this.js, variable.language.super.js",
			"settings": {
				"foreground": "#97AF96",
			}
		},

		// JavaScript others.
		{
			"scope": "variable.parameter.js",
			"settings": {
				"foreground": "#CC9292",
			}
		},

		// JavaScript string template.
		{
			"scope": "string.template.js, string.template.js variable.other.readwrite.js, string.template.js punctuation.definition.template-expression.begin.js, string.template.js punctuation.definition.template-expression.end.js, punctuation.definition.string.template.begin.js, punctuation.definition.string.template.end.js",
			"settings": {
				"foreground": "#C9B888",
			}
		},

		// JavaScript string template variables.
		{
			"scope": "string.template.js variable.other.readwrite.js, string.template.js punctuation.definition.template-expression.begin.js, string.template.js punctuation.definition.template-expression.end.js",
			"settings": {
				"foreground": "#CC9292",
			}
		},

		// JavaScript JSON object key and values (any way to simplify the scope?)
		{
			"scope": "meta.object.member.js, meta.object.member.js meta.object-literal.key.js string.quoted.single.js, meta.object.member.js meta.object-literal.key.js string.quoted.single.js punctuation.definition.string.begin.js, meta.object.member.js meta.object-literal.key.js string.quoted.single.js punctuation.definition.string.end.js, meta.object.member.js meta.object-literal.key.js string.quoted.double.js, meta.object.member.js meta.object-literal.key.js string.quoted.double.js punctuation.definition.string.begin.js, meta.object.member.js meta.object-literal.key.js string.quoted.double.js punctuation.definition.string.end.js, meta.object.member.js meta.object-literal.key.js meta.array.literal.js variable.other.readwrite.js",
			"settings": {
				"foreground": "#CC9292",
			}
		},

		// JavaScript JSON object (any way to simplify the scope?)
		{
			"scope": "meta.object.member.js string.quoted.single.js, meta.object.member.js string.quoted.double.js, meta.object.member.js constant.language.boolean.false.js, meta.object.member.js constant.language.boolean.true.js",
			"settings": {
				"foreground": "#B8B8B8",
			}
		},

		// Vue
		{
			"scope": "source.vue variable.other.property.js, source.vue variable.other.object.property.js",
			"settings": {
				"foreground": "#CC9292",
			}
		},

		// JSON specifics.
		{
			"scope": "constant.numeric.json, constant.language.json",
			"settings": {
				"foreground": "#B8B8B8",
			}
		},

		{
			"scope": "support.type.property-name.json, punctuation.support.type.property-name.begin.json, punctuation.support.type.property-name.end.json",
			"settings": {
				"foreground": "#AF92CC",
			}
		},

		// Java specifics.
		{
			"scope": "storage.type.annotation.java, storage.modifier.import.java, punctuation.definition.annotation.java",
			"settings": {
				"foreground": "#C9B888"
			}
		},
		{
			"scope": "storage.modifier.java",
			"settings": {
				"foreground": "#AF92CC"
			}
		},
		{
			"scope": "variable.parameter.java",
			"settings": {
				"foreground": "#CC9292"
			}
		},
		{
			"scope": "storage.type.java",
			"settings": {
				"foreground": "#88A2D3"
			}
		},

		// Markdown specifics.
		{
			"scope": "meta.image.inline.markdown, meta.link.inline.markdown",
			"settings": {
				"foreground": "#97AF96"
			}
		},
		
		// Flutter/Dart specifics.
		{
			"scope": "storage.type.annotation.dart",
			"settings": {
				"foreground": "#C9B888"
			}
		},

		// Python specifics.
		{
			"scope": "string.quoted.docstring.multi.python",
			"settings": {
				"foreground": "#97AF96"
			}
		},
		{
			"scope": "variable.parameter.function.language.python",
			"settings": {
				"foreground": "#CC9292"
			}
		}
	]
}